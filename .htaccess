<Files info.php>
 order allow,deny
 deny from all
 allow from 127.0.0.1
</Files>
<Files .htaccess>
 order allow,deny
 deny from all
 allow from 127.0.0.1
</Files>
addDefaultCharset utf-8

AuthType Basic
AuthName "Protected"
AuthUserFile /etc/phpmyadmin/.htpasswd
Require valid-user

RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f


RewriteRule (.*) index.php?url=$1 [L,QSA]